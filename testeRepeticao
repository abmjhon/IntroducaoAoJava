import java.util.Scanner;

public class testeRepeticao {
    public static Scanner leia = new Scanner(System.in);
    public static void main(String[] args){

    for(int i = 1; i<=10; i++){
        for(int j=1; j<=10; j++){
            System.out.println(i + " x " + j + " = " + i * j);
        }
    }

    }
}
